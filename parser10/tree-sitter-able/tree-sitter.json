{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "able",
      "camelcase": "Able",
      "title": "Able",
      "scope": "source.able",
      "file-types": [
        "able"
      ],
      "injection-regex": "^able$",
      "class-name": "TreeSitterAble"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "epl-2.0",
    "description": "Able language parser (v10 spec)",
    "authors": [
      {
        "name": "David Ellis",
        "email": "david@conquerthelawn.com"
      }
    ],
    "links": {
      "repository": "https://github.com/davidkellis/able"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}