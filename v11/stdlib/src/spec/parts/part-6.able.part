fn tag_list_contains(tags: Array string, needle: string) -> bool {
  idx := 0
  loop {
    if idx >= tags.len() { break }
    tags.get(idx) match {
      case nil => {},
      case value: string => {
        if value == needle { return true }
      }
    }
    idx = idx + 1
  }
  false
}

