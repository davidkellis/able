package app

dynimport dynimport_parity.catalog::catalog
dynimport dynimport_parity.pricing.{ price_for }

fn describe(item: String) -> String {
  `  ${item}: ${price_for(item)}`
}

fn main() {
  print("menu")
  for item in catalog.list_items() {
    print(describe(item))
  }
  print(`featured ${catalog.featured_label()}`)
}
