package app

dynimport extras.catalog::catalog
dynimport extras.pricing.{ price_for }

fn describe(item: string) -> string {
  `  ${item}: ${price_for(item)}`
}

fn main() {
  print("multi-root menu")
  for item in catalog.list_items() {
    print(describe(item))
  }
  print(`featured ${catalog.featured()}`)
}
