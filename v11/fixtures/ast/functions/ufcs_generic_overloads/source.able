struct Box <T> {
  value: T
}
fn describe<T>(box: Box T) -> i32 {
  1;
}
fn describe(box: Box i32) -> i32 {
  box.value + 1;
}
fn sum(box: Box i32, delta: i32) -> i32 {
  box.value + delta;
}
intBox := Box<i32> { value: 5 };
boolBox := Box<bool> { value: false };
[intBox.describe(), boolBox.describe(), intBox |> describe, sum(intBox, 2), intBox.sum(3), intBox |> sum(4), boolBox |> describe];
