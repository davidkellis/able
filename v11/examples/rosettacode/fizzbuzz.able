package rosettacode_fizzbuzz

## Rosetta Code â€” FizzBuzz (https://rosettacode.org/wiki/FizzBuzz)
fn fizzbuzz_label(value: i32) -> string {
  if value %% 15 == 0 { "FizzBuzz" }
  or value %% 3 == 0 { "Fizz" }
  or value %% 5 == 0 { "Buzz" }
  or { `${value}` }
}

fn fizzbuzz(limit: i32) -> void {
  for n in 1..limit {
    print(fizzbuzz_label(n))
  }
}

fn main() -> void {
  fizzbuzz(100)
}
