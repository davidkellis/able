interface Show {
  fn to_string(self: Self) -> String;
}

struct Point {
  x: i32,
  y: i32
}

methods Point {
  fn to_string(self: Point) -> String {
    return `Point(${self.x}, ${self.y})`;
  }

  fn accept_show<T: Show>(self: Point, x: T) -> String {
    return x.to_string();
  }
}

p := Point { x: 1, y: 2 }
p.accept_show<i32>(3)
