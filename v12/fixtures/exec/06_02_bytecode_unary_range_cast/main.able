package exec_06_02_bytecode_unary_range_cast

## Semantics: unary ops, ranges, type casts, and interpolation execute in bytecode mode.
fn main() -> void {
  print(-3)
  print(!false)

  values := 1..4
  i := 0
  second := 0
  for v in values {
    if i == 0 || i == 2 { print(v) }
    if i == 1 { second = v }
    i = i + 1
  }

  casted := 3.7 as i32
  print(casted)
  print(`sum ${casted + second}`)
}
